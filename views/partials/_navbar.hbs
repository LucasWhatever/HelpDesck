<header class="bg-dark fixed-top d-flex justify-content-between align-items-center gap-5"
  style="box-shadow: 0px 2px 5px black;height: 5em;">
  {{#if funcionarioentrando }}
  <img src="https://res.cloudinary.com/daxkdqdex/image/upload/v1660951450/senai_logo_gk0jhc_j1fi6i.png"
    alt="logo do fiesc sesi" class="ms-3">
  {{#each funcionarioentrando }}
  <button class="navbar-toggler btn btn-outline-light fw-bold" type="button" data-bs-toggle="offcanvas"
    data-bs-target="#offcanvasNavbar" aria-controls="offcanvasNavbar">
    Precisa De Ajuda?
  </button>
  <ul class="d-flex flex-row align-items-center mt-4 gap-2" style="list-style:none ;">
    <li class="mt-3">
      <p class="text-white fw-bold">Bem-Vindo(a) {{Nome}}</p>
    </li>
    <li class="me-3">
